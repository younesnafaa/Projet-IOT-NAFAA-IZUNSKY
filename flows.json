    {
        "id": "e38a6e103b1671c6",
        "type": "function",
        "z": "5afb1467026d4594",
        "name": "ECG alert logic",
        "func": "var ecg = Number(msg.payload.ecg);\nvar status = msg.payload.status || \"\";\nvar HIGH_LIMIT = 3500;\nvar LOW_LIMIT  = 500;\nvar alert = null;\nif (status === \"electrodes_off\") {\n    alert = \"Electrodes ECG déconnectées\";\n}\nelse if (!isNaN(ecg)) {\n    if (ecg > HIGH_LIMIT) {\n        alert = \"ECG anormal: valeur très élevée (\" + ecg + \")\";\n    } else if (ecg < LOW_LIMIT) {\n        alert = \"ECG anormal: valeur très basse (\" + ecg + \")\";\n    }\n}\nif (alert) {\n    return { payload: alert };\n}\nreturn null;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 540,
        "y": 110,
