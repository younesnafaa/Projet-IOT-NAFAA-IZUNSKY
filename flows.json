    {
        "id": "214eca040bc1e078",
        "type": "function",
        "z": "5afb1467026d4594",
        "name": "Extract counters per gesture",
        "func": "var counters = msg.payload.counters || {};\nvar msgs = [];\nfor (var name in counters) {\n    if (!counters.hasOwnProperty(name)) continue;\n    var lower = name.toLowerCase();\n    if (lower === \"none\") continue;\n    msgs.push({\n        topic: lower,\n        gesture: lower,\n        payload: counters[name]\n    });\n}\nreturn [msgs];",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 580,
        "y": 260,
        "wires": [
